{
  "name": "entangld",
  "version": "2.1.6",
  "description": "Synchronized key-value stores with RPCs and events",
  "main": "index.js",
  "jshintConfig": {
    "esversion": 6,
    "unused": true
  },
  "scripts": {
    "//": "DEV RULES",
    "test": "mocha",
    "lint:js": "jshint --reporter=node_modules/jshint-stylish index.js test/",
    "build": "npm run lint:js -s && npm run test -s && npm run docs -s",
    "watch:lint:js": "nodemon -w index.js -w test/ -x npm run lint:js -s",
    "dev": "nodemon -w index.js -w test/ -x npm run build -s",
    "docs": "node generate_docs.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DaxBot/entangld"
  },
  "author": "c0d3",
  "contributors": [
    "Wilkins White <wilkins.white@daxbot.com>",
    "Jonathan Van Schenck <jon.vanschenck@daxbot.com>"
  ],
  "license": "MIT",
  "homepage": "https://daxbot.com/open-source",
  "devDependencies": {
    "fs-then-native": "^2.0.0",
    "jsdoc-to-markdown": "^7.1.1",
    "jshint": "^2.13.1",
    "jshint-stylish": "^2.2.1",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "sockhop": "^1.4.3"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  }
}
